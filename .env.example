# Qiita API Configuration
# Get your access token from: https://qiita.com/settings/applications
QIITA_ACCESS_TOKEN=your_qiita_access_token_here

# OpenAI API Configuration
OPENAI_API_KEY=your_openai_api_key_here

# Optional: Custom API base URL
# OPENAI_BASE_URL=https://api.openai.com/v1

# Other LLM APIs (optional)
ANTHROPIC_API_KEY=your_anthropic_api_key_here
GOOGLE_API_KEY=your_google_api_key_here

# Optional: LangSmith Configuration for monitoring
# LANGCHAIN_TRACING_V2=true
# LANGCHAIN_API_KEY=your_langchain_api_key_here
# LANGCHAIN_PROJECT=autopoc